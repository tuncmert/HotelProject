@model List<StaffViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
<table class="table table-bordered">
       <thead>
           <tr>
            <th>#</th>
            <th>Personel Adı</th>
            <th>Personel Ünvan</th>
            <th>Sil</th>
            <th>Güncelle</th>
            <th>Detaylar</th>
           </tr>
       </thead>
       <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <th>@item.StaffID</th>
                <td>@item.Name</td>
                <td>@item.Title</td>
                <td><a href="/Staff/DeleteStaff/@item.StaffID" class="btn btn-rounded btn-outline-danger">Sil</a></td>
                <td><a href="/Staff/UpdateStaff/@item.StaffID" class="btn btn-rounded btn-outline-success">Güncelle</a></td>
                <td><a href="#" class="btn btn-rounded btn-outline-primary">Detaylar</a></td>
            </tr>
        }
    </tbody>
</table>

<a href="/Staff/AddStaff" class="btn btn-info">Yeni Personel</a>

