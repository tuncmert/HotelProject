@model List<ResultAboutDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>#</th>
            <th>Başlık 1</th>
            <th>Başlık 2</th>
            <th>Güncelle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>@item.AboutID</th>
                <td>@item.Title1</td>
                <td>@item.Title2</td>
                <td><a href="/AdminAbout/UpdateAbout/@item.AboutID" class="btn btn-rounded btn-outline-success">Güncelle</a></td>
            </tr>
        }
    </tbody>
</table>


